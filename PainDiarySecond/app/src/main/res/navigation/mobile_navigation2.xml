<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation2"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.paindiarysecond.ui.reports.LocationChartFragment"
        android:label="Location Chart"
        tools:layout="@layout/fragment_location_chart" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.paindiarysecond.ui.reports.StepsChartFragment"
        android:label="Steps Chart"
        tools:layout="@layout/fragment_steps_chart" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.paindiarysecond.ui.reports.LineChartFragment"
        android:label="Line Chart"
        tools:layout="@layout/fragment_line_chart" />
</navigation>